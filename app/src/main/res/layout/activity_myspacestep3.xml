<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_myspacestep3"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageButton
        android:id="@+id/back"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@color/transparent"
        android:src="@drawable/back"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/animationView"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="@+id/progressBar"
        app:lottie_rawRes="@raw/myspace_step"
        app:lottie_autoPlay="true"
        app:lottie_loop="true" />

    <com.google.android.material.progressindicator.LinearProgressIndicator
        android:id="@+id/progressBar"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:layout_marginStart="80dp"
        android:max="100"
        android:progress="25"
        app:trackColor="@color/gray"
        app:trackThickness="15dp"
        app:trackCornerRadius="100dp"
        app:indicatorColor="@color/icon_aboutme_background"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/myspace_step3_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="@string/myspace_step3_main"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar" />

    <TextView
        android:id="@+id/myspace_step3_sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="@string/myspace_step3_sub"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.398"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step3_main" />

    <TextView
        android:id="@+id/myspace_step3_sub2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/gmarketsanslight"
        android:text="@string/myspace_step3_sub2"
        app:layout_constraintEnd_toEndOf="@+id/myspace_step3_sub"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/myspace_step3_sub"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step3_sub" />

    <ImageView
        android:id="@+id/myspace_step2_room_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step2_room"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@id/myspace_step3_sub2"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step3_sub2" />

    <ImageView
        android:id="@+id/room_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step3_smallroom_1"
        app:layout_constraintBottom_toBottomOf="@+id/myspace_step2_room_1"
        app:layout_constraintEnd_toEndOf="@+id/myspace_step2_room_1"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_1"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_1"
        app:layout_constraintVertical_bias="1.0" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_1"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_1">

        <ImageView
            android:id="@+id/checkbox_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkspace"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp" />

        <ImageView
            android:id="@+id/checkmark_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkmark"
            android:visibility="invisible"/>

    </RelativeLayout>

    <ImageView
        android:id="@+id/myspace_step2_room_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step2_room"
        android:layout_marginTop="40dp"
        android:layout_marginStart="30dp"
        app:layout_constraintStart_toEndOf="@id/myspace_step2_room_1"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step3_sub2" />

    <ImageView
        android:id="@+id/room_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step3_smallroom_2"
        app:layout_constraintBottom_toBottomOf="@+id/myspace_step2_room_2"
        app:layout_constraintEnd_toEndOf="@+id/myspace_step2_room_2"
        app:layout_constraintHorizontal_bias="0.595"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_2"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_2"
        app:layout_constraintVertical_bias="0.0" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_2"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_2">

        <ImageView
            android:id="@+id/checkbox_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkspace"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp" />

        <ImageView
            android:id="@+id/checkmark_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkmark"
            android:visibility="invisible"/>

    </RelativeLayout>

    <ImageView
        android:id="@+id/myspace_step2_room_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step2_room"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@id/myspace_step2_room_1"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step2_room_1" />

    <ImageView
        android:id="@+id/room_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step3_smallroom_3"
        app:layout_constraintBottom_toBottomOf="@+id/myspace_step2_room_3"
        app:layout_constraintEnd_toEndOf="@+id/myspace_step2_room_3"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_3"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_3"
        app:layout_constraintVertical_bias="1.0" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_3"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_3">

        <ImageView
            android:id="@+id/checkbox_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkspace"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp" />

        <ImageView
            android:id="@+id/checkmark_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkmark"
            android:visibility="invisible"/>

    </RelativeLayout>

    <ImageView
        android:id="@+id/myspace_step2_room_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step2_room"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@id/myspace_step2_room_2"
        app:layout_constraintTop_toBottomOf="@+id/myspace_step2_room_2" />

    <ImageView
        android:id="@+id/room_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/step3_smallroom_4"
        app:layout_constraintBottom_toBottomOf="@+id/myspace_step2_room_4"
        app:layout_constraintEnd_toEndOf="@+id/myspace_step2_room_4"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_4"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_4"
        app:layout_constraintVertical_bias="1.0" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/myspace_step2_room_4"
        app:layout_constraintTop_toTopOf="@+id/myspace_step2_room_4">

        <ImageView
            android:id="@+id/checkbox_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkspace"
            android:layout_marginTop="10dp"
            android:layout_marginStart="5dp" />

        <ImageView
            android:id="@+id/checkmark_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/step2_checkmark"
            android:visibility="invisible"/>

    </RelativeLayout>

    <ImageButton
        android:id="@+id/next_ib_step3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="80dp"
        android:background="@color/transparent"
        android:src="@drawable/complete_korean"
        app:layout_constraintStart_toEndOf="@id/myspace_step2_room_3"
        app:layout_constraintTop_toBottomOf="@id/myspace_step2_room_3"
        android:visibility="invisible"/>

    <ImageView
        android:id="@+id/next_ib_step3_click_area"
        android:layout_width="200dp"
        android:layout_height="70dp"
        app:layout_constraintTop_toTopOf="@+id/next_ib_step3"
        app:layout_constraintBottom_toBottomOf="@+id/next_ib_step3"
        app:layout_constraintStart_toStartOf="@+id/next_ib_step3"
        app:layout_constraintEnd_toEndOf="@+id/next_ib_step3"
        android:onClick="onClick"/>


</androidx.constraintlayout.widget.ConstraintLayout>