<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/search_share_bg1"
    tools:context=".Search.CustomDialogProf">

    <CheckBox
        android:id="@+id/cb1"
        android:layout_width="85dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="프로필 1"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/cb_iv1"
        app:layout_constraintStart_toStartOf="@+id/cb_iv1"
        app:layout_constraintTop_toTopOf="@+id/cb_iv1" />

    <CheckBox
        android:id="@+id/cb2"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="프로필 2"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/cb_iv2"
        app:layout_constraintStart_toStartOf="@+id/cb_iv2"
        app:layout_constraintTop_toTopOf="@+id/cb_iv2" />

    <CheckBox
        android:id="@+id/cb3"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="프로필 3"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/cb_iv3"
        app:layout_constraintStart_toStartOf="@+id/cb_iv3"
        app:layout_constraintTop_toTopOf="@+id/cb_iv3" />

    <ImageView
        android:id="@+id/cb_iv1"
        android:layout_width="59dp"
        android:layout_height="112dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="60dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toStartOf="@+id/cb_iv2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/searchspace_card2" />

    <ImageView
        android:id="@+id/cb_iv2"
        android:layout_width="59dp"
        android:layout_height="112dp"
        android:layout_marginTop="10dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/searchspace_card2" />

    <ImageView
        android:id="@+id/cb_iv3"
        android:layout_width="59dp"
        android:layout_height="112dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="10dp"
        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/cb_iv2"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/searchspace_card2" />

    <Button
        android:id="@+id/yes_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="152dp"
        android:layout_marginBottom="10dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="공유"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>