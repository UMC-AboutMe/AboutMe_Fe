<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_myspacemain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:clickable="true">

<androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="71dp" />

    <ImageView
        android:id="@+id/member_logo"
        android:layout_width="130dp"
        android:layout_height="80dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/home_logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/member_myspace_title_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/myspace_title"
        app:layout_constraintStart_toStartOf="@+id/member_logo"
        app:layout_constraintTop_toBottomOf="@+id/member_logo" />

    <TextView
        android:id="@+id/member_myspace_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:fontFamily="@font/gmarketsansmedium"
        android:text="Member"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/member_myspace_title_line"
        app:layout_constraintStart_toEndOf="@+id/member_myspace_title_line"
        app:layout_constraintTop_toTopOf="@id/member_myspace_title_line" />


    <ImageView
        android:id="@+id/member_step3_selected_room"
        android:layout_width="354dp"
        android:layout_height="341dp"
        android:layout_marginTop="50dp"
        android:foregroundGravity="center"
        android:src="@drawable/step3_smallroom_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/member_myspace_title_line" />

    <ImageButton
        android:id="@+id/member_step3_feeling"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_feeling"
        app:layout_constraintBottom_toTopOf="@+id/member_step3_selected_avatar"
        app:layout_constraintEnd_toEndOf="@+id/member_step3_selected_room"
        app:layout_constraintStart_toStartOf="@+id/member_step3_selected_room"
        android:foreground="?attr/selectableItemBackground" />

    <ImageButton
        android:id="@+id/member_step3_music"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_music"
        app:layout_constraintBottom_toTopOf="@+id/member_step3_story"
        app:layout_constraintStart_toEndOf="@+id/member_step3_feeling"
        android:foreground="?attr/selectableItemBackground" />

    <ImageButton
        android:id="@+id/member_step3_photo"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="20dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_photo"
        app:layout_constraintBottom_toTopOf="@+id/member_step3_story"
        app:layout_constraintStart_toStartOf="@id/member_step3_story"
        android:foreground="?attr/selectableItemBackground" />

    <ImageButton
        android:id="@+id/member_step3_story"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="12dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_story"
        app:layout_constraintStart_toEndOf="@+id/member_step3_selected_avatar"
        app:layout_constraintTop_toTopOf="@+id/member_step3_selected_avatar"
        android:foreground="?attr/selectableItemBackground" />

    <ImageButton
        android:id="@+id/member_step3_schedule"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="20dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_schedule"
        app:layout_constraintBottom_toTopOf="@+id/member_step3_comment"
        app:layout_constraintEnd_toStartOf="@+id/member_step3_comment"
        android:foreground="?attr/selectableItemBackground" />

    <ImageButton
        android:id="@+id/member_step3_comment"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="10dp"
        android:background="@color/transparent"
        android:scaleType="fitCenter"
        android:src="@drawable/step3_comment"
        app:layout_constraintBottom_toBottomOf="@+id/member_step3_selected_avatar"
        app:layout_constraintEnd_toStartOf="@+id/member_step3_feeling"
        android:foreground="?attr/selectableItemBackground" />

    <ImageView
        android:id="@+id/member_step3_selected_avatar"
        android:layout_width="63dp"
        android:layout_height="72dp"
        android:src="@drawable/step2_avatar_1"
        app:layout_constraintBottom_toBottomOf="@+id/member_step3_selected_room"
        app:layout_constraintEnd_toEndOf="@+id/member_step3_selected_room"
        app:layout_constraintStart_toStartOf="@+id/member_step3_selected_room"
        app:layout_constraintTop_toTopOf="@+id/member_step3_selected_room" />

</androidx.constraintlayout.widget.ConstraintLayout>