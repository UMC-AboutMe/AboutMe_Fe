<?xml version="1.0" encoding="utf-8"?>
<layout>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:background="@drawable/backprofile_img">

        <ImageButton
            android:id="@+id/turn_btn2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/myprofile_turn"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="90dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="270dp" />

        <ImageView
            android:layout_width="150dp"
            android:layout_height="100dp"
            android:src="@drawable/myprofile_logo"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="50dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>


        <ImageView
            android:id="@+id/recommend_et1_iv"
            android:layout_width="100dp"
            android:layout_height="35dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="150dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/backProfile_et1"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/gmarketsansmedium"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="180dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="50dp" />

            <ImageButton
                android:id="@+id/profileEdit1_btn"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/myprofile_pencil"
                android:layout_marginTop="190dp"
                android:layout_marginLeft="110dp" />

        </RelativeLayout>

        <ImageView
            android:id="@+id/recommend_et2_iv"
            android:layout_width="100dp"
            android:layout_height="35dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="230dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/backProfile_et2"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/gmarketsansmedium"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="260dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="50dp" />

            <ImageButton
                android:id="@+id/profileEdit2_btn"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/myprofile_pencil"
                android:layout_marginTop="270dp"
                android:layout_marginLeft="110dp" />

        </RelativeLayout>

        <ImageView
            android:id="@+id/recommend_et3_iv"
            android:layout_width="100dp"
            android:layout_height="35dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="310dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">


            <TextView
                android:id="@+id/backProfile_et3"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginBottom="24dp"
                android:fontFamily="@font/gmarketsansmedium"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="340dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="50dp" />

            <ImageButton
                android:id="@+id/profileEdit3_btn"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/myprofile_pencil"
                android:layout_marginTop="350dp"
                android:layout_marginLeft="110dp"/>

        </RelativeLayout>

        <ImageView
            android:id="@+id/recommend_et4_iv"
            android:layout_width="100dp"
            android:layout_height="35dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="390dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/backProfile_et4"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginBottom="24dp"
                android:ems="12"
                android:hint="내용4"
                android:fontFamily="@font/gmarketsansmedium"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="420dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="50dp"
                android:background="@color/transparent"/>

            <ImageButton
                android:id="@+id/profileEdit4_btn"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/myprofile_pencil"
                android:layout_marginTop="430dp"
                android:layout_marginLeft="110dp"/>

        </RelativeLayout>



        <ImageView
            android:id="@+id/recommend_et5_iv"
            android:layout_width="100dp"
            android:layout_height="35dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="470dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="50dp"/>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <EditText
                android:id="@+id/backProfile_et5"
                android:layout_width="250dp"
                android:layout_height="50dp"
                android:layout_marginBottom="24dp"
                android:ems="12"
                android:hint="내용5"
                android:fontFamily="@font/gmarketsansmedium"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="500dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="50dp"
                android:background="@color/transparent"/>

            <ImageButton
                android:id="@+id/profileEdit5_btn"
                android:layout_width="25dp"
                android:layout_height="25dp"
                android:background="@drawable/myprofile_pencil"
                android:layout_marginTop="510dp"
                android:layout_marginLeft="110dp"/>

        </RelativeLayout>



    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>